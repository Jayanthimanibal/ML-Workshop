print(df.head())
print(df.dtypes)

horsepower_mean=0
for v in df['horsepower'].values.ravel():
    try:
        horsepower_mean +=int(v)
    except:
        pass

horsepower_mean=horsepower_mean/len(df)
df['horsepower'].replace({'?':horsepower_mean},inplace=True)
df['horsepower'] =df['horsepower'].astype(int)
print(df.dtypes)
